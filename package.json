{
  "name": "soulseek-ts",
  "version": "2.0.6",
  "description": "A Soulseek client for Node",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgchk/soulseek-ts.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "prettier --check . && tsc --noEmit && eslint .",
    "format": "prettier --write .",
    "test": "pnpm run test:basic",
    "test:basic": "tsx examples/basic.ts",
    "test:minimal": "tsx examples/minimal.ts",
    "test:resume": "tsx examples/resume.ts"
  },
  "keywords": [
    "soulseek",
    "slsk"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.6",
    "tsup": "^6.6.3",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "typed-emitter": "^2.1.0",
    "zlib": "^1.0.5"
  }
}
